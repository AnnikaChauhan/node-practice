<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Profile | <%= person %></title>
    <link href="/assets/styles.css" rel="stylesheet" type="text/css">
</head>
<body>
    <%- include('partials/nav') -%>
    <h1><%= person %>'s page</h1>
    <p>Welcome!</p>
    <p>Age: <%= data.age %></p>
    <p>Job: <%= data.job %></p>
    <h2>Hobbies</h2>
    <ul>
        <% data.hobbies.forEach((hobby) => {%>
            <li><%= hobby %></li>
        <%});%>
    </ul>
</body>
</html>