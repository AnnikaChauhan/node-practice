<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Home</title>
    <style>
        body{
            background-color: cadetblue;
            color: white;
        }
    </style>
</head>
<body>
    <h1><%= person %>'s page</h1>
    <p>Welcome!</p>
    <p>Age: <%= data.age %></p>
    <p>Job: <%= data.job %></p>
    <h2>Hobbies</h2>
    <ul>
        <% data.hobbies.forEach((hobby) => {%>
            <li><%= hobby %></li>
        <%});%>
    </ul>
</body>
</html>